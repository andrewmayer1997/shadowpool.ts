{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es2018",
    "module": "commonjs",
    "lib": ["esnext"],
    "allowJs": true,
    "outDir": "build",
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": ["src-old", "watch"],
  "exclude": ["src-old/core/view/front"]
}
